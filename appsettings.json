{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Keycloak": {
    "Authority": "__KeycloakAuthority__",
    "ClientId": "__KeycloakClientId__",
    "ClientSecret": "__KeycloakClientSecret__",
    "ResponseType": "code",
    "SaveTokens": true,
    "Scope": "openid",
    "CallbackPath": "/login-callback",
    "SignedOutCallbackPath": "/logout-callback"
  }
}
